generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["nativeTypes"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String    @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique
  name String?
  password String?
  salt String
}
